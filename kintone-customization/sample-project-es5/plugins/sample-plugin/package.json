    "@cybozu/eslint-config": "14.0.0",
    "@kintone/plugin-packer": "5.0.3",
    "@kintone/plugin-uploader": "5.0.2",
    "npm-run-all": "4.1.5",
    "eslint": "7.27.0"
  }
}
